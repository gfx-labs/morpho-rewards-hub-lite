{
  "name": "cli",
  "author": "Morpho Association <contact@morpho.org>",
  "packageManager": "yarn@4.1.1",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "build:cli": "turbo run build",
    "cli:run": "tsx src/index.ts",
    "test": "vitest --run",
    "test:cli": "turbo run test"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@vitest/coverage-v8": "^3.0.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.4.5",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "tslog": "^4.9.2",
    "viem": "^2.22.8"
  }
}