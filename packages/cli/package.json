{
  "name": "@morpho-org/cli",
  "author": "Morpho Association <contact@morpho.org>",
  "packageManager": "yarn@4.1.1",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:morpho-prices": "turbo run test",
    "build": "tsup",
    "build:morpho-prices": "turbo run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@vitest/coverage-v8": "^3.0.2",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.1.6",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "viem": "^2.22.8"
  }
}
